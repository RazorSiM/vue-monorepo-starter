{
  "name": "ui",
  "version": "0.0.0",
  "main": "./dist/ui.umd.js",
  "module": "./dist/ui.mjs",
  "types": "./dist/src/main.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/ui.mjs",
      "require": "./dist/ui.umd.js"
    },
    "./index.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest run --environment jsdom --root src/",
    "test:unit:dev": "vitest --environment jsdom --root src/",
    "test:component": "cypress run --component",
    "test:component:dev": "cypress open --component",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "prettier:check": "prettier --check . --ignore-path .prettierignore",
    "prettier:write": "prettier --write . --ignore-path .prettierignore"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@unocss/preset-icons": "^0.49.4",
    "@unocss/preset-uno": "^0.49.4",
    "@unocss/preset-web-fonts": "^0.49.4",
    "@unocss/reset": "^0.49.4",
    "@unocss/transformer-variant-group": "^0.49.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^12.0.2",
    "eslint-config-vue": "workspace:*",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^1.15.2",
    "typescript": "~4.7.4",
    "unocss": "^0.49.4",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.2",
    "vitest": "^0.25.6",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^1.0.12"
  }
}

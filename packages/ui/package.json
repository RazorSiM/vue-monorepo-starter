{
  "name": "ui",
  "version": "0.0.0",
  "main": "./dist/ui.umd.js",
  "module": "./dist/ui.mjs",
  "types": "./dist/src/main.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/ui.mjs",
      "require": "./dist/ui.umd.js"
    },
    "./index.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest run --environment jsdom --root src/",
    "test:unit:dev": "vitest --environment jsdom --root src/",
    "test:component": "cypress run --component",
    "test:component:dev": "cypress open --component",
    "build-only": "vite build --config vite-library.config.ts",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "prettier:check": "prettier --check . --ignore-path .prettierignore",
    "prettier:write": "prettier --write . --ignore-path .prettierignore",
    "storybook:dev": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@vueuse/core": "^9.12.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.18",
    "@storybook/addon-essentials": "^7.0.0-beta.45",
    "@storybook/addon-interactions": "^7.0.0-beta.45",
    "@storybook/addon-links": "^7.0.0-beta.45",
    "@storybook/blocks": "^7.0.0-beta.45",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/vue3": "^7.0.0-beta.45",
    "@storybook/vue3-vite": "^7.0.0-beta.45",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.13.0",
    "@unocss/preset-icons": "^0.49.4",
    "@unocss/preset-typography": "^0.49.4",
    "@unocss/preset-uno": "^0.49.4",
    "@unocss/preset-web-fonts": "^0.49.4",
    "@unocss/reset": "^0.49.4",
    "@unocss/transformer-variant-group": "^0.49.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/test-utils": "^2.2.10",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^12.5.1",
    "eslint-config-vue": "workspace:*",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "start-server-and-test": "^1.15.3",
    "storybook": "^7.0.0-beta.45",
    "typescript": "~4.9.5",
    "unocss": "^0.49.4",
    "vite": "^4.1.1",
    "vite-plugin-dts": "^1.7.2",
    "vitepress": "1.0.0-alpha.46",
    "vitest": "^0.28.4",
    "vue-loader": "^17.0.1",
    "vue-tsc": "^1.0.24"
  }
}
